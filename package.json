{
  "name": "tm4",
  "version": "1.0.0",
  "description": "TM4 Hugo Theme",
  "private": true,
  "scripts": {
    "dev": "hugo server --config config/_default/config.development.toml --environment development -D",
    "format": "npx prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,html}\"",
    "clean": "npx rimraf public resources node_modules",
    "prebuild": "npm run clean",
    "prod": "hugo --config config/_default/config.toml --environment production --minify",
    "dev:build": "hugo --config config/_default/config.development.toml --environment development"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
